{
  "name": "tree-iteration",
  "version": "0.2.1",
  "description": "Tree structure with Zipper iteration",
  "type": "module",
  "keywords": [
    "tree",
    "node",
    "zipper"
  ],
  "author": "Marco van Kampen <m.kampen@hotmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "dist": "dist",
    "tests": "tests"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./zipper": {
      "types": "./dist/zipper.d.ts",
      "default": "./dist/zipper.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "default": "./dist/node.js"
    }
  },
  "scripts": {
    "lint": "eslint",
    "test": "jest",
    "build": "tsc --build",
    "clean": "tsc --build --clean && rm ./dist/*.d.ts"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvkampen/tree-iteration.git"
  },
  "bugs": {
    "url": "https://github.com/mvkampen/tree-iteration/issues"
  },
  "homepage": "https://github.com/mvkampen/tree-iteration#readme",
  "private": "true",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/jest": "^29.5.12",
    "eslint": "^9.2.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {}
}
