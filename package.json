{
  "name": "tree-iteration",
  "version": "0.2.1",
  "description": "Tree structure with Zipper iteration",
  "keywords": [
    "tree",
    "node",
    "zipper"
  ],
  "author": "Marco van Kampen <m.kampen@hotmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "directories": {
    "dist": "dist",
    "tests": "tests"
  },
  "scripts": {
    "lint": "eslint",
    "test": "jest",
    "build": "npx esbuild --config 'esbuild.config.ts'",
    "clean": "rm -rf ./dist"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.mjs"
    },
    "./*": {
      "import": "./*.mjs",
      "require": "./*.cjs",
      "default": "./*.mjs"
    }
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvkampen/tree-iteration.git"
  },
  "bugs": {
    "url": "https://github.com/mvkampen/tree-iteration/issues"
  },
  "homepage": "https://github.com/mvkampen/tree-iteration#readme",
  "private": "true",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/jest": "^29.5.12",
    "esbuild": "^0.21.1",
    "eslint": "^9.2.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {}
}
